{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "module": "ES2020",
        "moduleResolution": "node",
        "target": "es6",
        "allowJs": true,
        "esModuleInterop": true, /* dayjs*/
        "allowSyntheticDefaultImports": true, /* dayjs*/
    },
    "include": [
        "./src/**/*"
    ]
}